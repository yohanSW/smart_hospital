#include "nurse.h"


using namespace han
{
	Nurse();
	int Nurse::danger_emergency(){
		
		digitalWrite(LED_RED, HIGH);
		return danger_num = 3;
	}

	int Nurse::danger_ringer(){

		digitalWrite(LED_YELLOW, HIGH);
		return danger_num = 3;
	}
	
	int Nurse::danger_situation_off(){
		
		digitalWrite(LED_RED, LOW);
		digitalWrite(LED_YELLOW, LOW);

		return danger_num = 0;
	}
}